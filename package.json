{
  "name": "json2x",
  "version": "0.0.1",
  "description": "Use your JSON data to generate models for various things",
  "main": "build/index.js",
  "keywords": [
    "json",
    "tool",
    "schema",
    "protobuf",
    "jsonschema",
    "swagger",
    "graphql"
  ],
  "scripts": {
    "build": "babel src -d build",
    "prepublish": "npm run build",
    "mock": "babel-node src/mock.js"
  },
  "bin": {
    "json2x": "build/json2x.js"
  },
  "files": [
    "build/index.js",
    "build/json2x"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konsumer/json2x.git"
  },
  "author": "David Konsumer <konsumer@jetboystudio.com> (http://konsumer.js.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konsumer/json2x/issues"
  },
  "homepage": "https://github.com/konsumer/json2x#readme",
  "dependencies": {
    "capitalize": "^1.0.0",
    "glob-require": "0.0.2",
    "json-schema-generator": "^2.0.6",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "faker": "^4.1.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "shebang"
    ]
  }
}
